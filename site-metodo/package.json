  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,mdx,css,scss}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,mdx,css,scss}\"",
    "type-check": "tsc --noEmit -p tsconfig.json",
    "quality": "npm run lint && npm run format:check && npm run type-check",
    "quality:fix": "npm run lint:fix && npm run format",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:seed": "ts-node --compiler-options '{\"module\":\"commonjs\"}' ./prisma/seeds/seed.ts",
    "analyze": "cross-env ANALYZE=true npm run build",
    "postinstall": "prisma generate --schema=./prisma/schema.prisma && prisma db seed --schema=./prisma/schema.prisma",
    "release": "standard-version --commit-all",
    "postrelease": "git push --follow-tags origin main",
    "test": "jest --passWithNoTests",
    "studio": "prisma studio --schema=./prisma/schema.prisma",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{json,md,mdx,css,scss}": [
      "prettier --write",
      "git add"
    ]
  },
